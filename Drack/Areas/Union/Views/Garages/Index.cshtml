@model IEnumerable<Drack.Models.Garage>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GarageName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GarageNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DriverDue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GarageName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GarageNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DriverDue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.GarageId }) |
            @Html.ActionLink("Details", "Details", new { id=item.GarageId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.GarageId })
        </td>
    </tr>
}

</table>
